{
  "name": "@examples/api",
  "description": "Example of interacting with Web Messenger deployment using Vitest, TS and JS",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.json  {src,__tests__}/**/*.ts && yarn check:style",
    "check:style": "prettier {src,__tests__} --check",
    "test": "vitest __tests__",
    "test:js": "node ../api/src/js-script.js",
    "test:ts": "node -r ts-node/register ../api/src/ts-script.ts"
  },
  "devDependencies": {
    "@makingchatbots/genesys-cloud-chatbot-tester": "2.0.7",
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "dotenv": "^16.3.1",
    "eslint": "latest",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
